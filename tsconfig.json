{
  "compilerOptions": {
    "target": "es2017",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "jsx": "react",
    "paths": {
      "@/*": ["src/*"],
      "components/*": ["src/components/*"],
      "api/*": ["src/api/*"],
      "utils/*": ["src/utils/*"],
      "generate/*": ["src/views/generate/*"]
    }
  },
  "exclude": ["node_modules", "dist", "build"],
  "include": ["src"]
}
